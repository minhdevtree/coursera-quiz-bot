{
  "manifest_version": 3,
  "name": "Coursera Quiz Bot",
  "version": "2.0",
  "description": "an extension that automatically answers coursera quizzes using google bard api",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "action": {
    "default_title": "Action Button"
  }
}
